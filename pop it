local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("DUPE BY NICO", "Ocean")

-- MAINS
local MAINS = Window:NewTab("DUPE")
local Section = MAINS:NewSection("Disfruta del dupe ;)")


-- BUTTONS AND OTHERS
Section:NewToggle("DUPE PIANO", "Crashea el server con los pianos", function(state)
    if state then
        _G.dupe = true
while _G.dupe == true do
local args = {
    [1] = "Piano"
}

game:GetService("ReplicatedStorage").RemoteEvents.Equip:FireServer(unpack(args))
wait()
end
    else
        _G.dupe = false
while _G.dupe == true do
local args = {
    [1] = "Piano"
}

game:GetService("ReplicatedStorage").RemoteEvents.Equip:FireServer(unpack(args))
wait()
end
    end
end)

Section:NewToggle("Auto Accept", "Auto Acepta las cosas", function(state)
    if state then
        _G.autoAccept = true
while _G.autoAccept == true do
-- Script generated by SimpleSpy - credits to exx#9394

game:GetService("ReplicatedStorage").RemoteEvents.Jumped:FireServer()
wait()
end
    else
        _G.autoAccept = false
while _G.autoAccept == true do
-- Script generated by SimpleSpy - credits to exx#9394

game:GetService("ReplicatedStorage").RemoteEvents.Jumped:FireServer()
wait()
end
    end
end)



Section:NewToggle("Auto Drop The Bananas", "Auto Dropea The Bananas", function(state)
    if state then
        _G.autoDropBananas = true
while _G.autoDropBananas == true do
local args = {
    [1] = "The banana"
}

game:GetService("ReplicatedStorage").RemoteEvents.Equip:FireServer(unpack(args))
wait(0.4)
-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "The banana"
}

game:GetService("ReplicatedStorage").RemoteEvents.Drop:FireServer(unpack(args))
wait(0.4)
end
    else
        _G.autoDropBananas = false
while _G.autoDropBananas == true do
local args = {
    [1] = "The banana"
}

game:GetService("ReplicatedStorage").RemoteEvents.Equip:FireServer(unpack(args))
wait(0.4)
-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "The banana"
}

game:GetService("ReplicatedStorage").RemoteEvents.Drop:FireServer(unpack(args))
wait(0.4)
end
    end
end)


-- OTROS
local OTROS = Window:NewTab("OTROS")
local OTROSSection = OTROS:NewSection("Scripts variados")

OTROSSection:NewButton("Infinity Yield", "Infinity Yield script", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

OTROSSection:NewSlider("WalkSpeed", "Cambia tu WalkSpeed", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
